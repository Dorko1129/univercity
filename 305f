#include <stdio.h>

int main()
{
    
    int tomb[10]={1, 2, 3, 4, 11, 22, 33, 44, 99, 10};
    int meret= 10;
    for (int i = 0; i < meret; i++)
    {
        if (meret-1==i)
        {
            printf("%d",tomb[i]);
        }
        else
        {
            printf("%d, ",tomb[i]);
        }

    }
    printf("\n");

}
